<!DOCTYPE html>
<html lang="en">


<head>
	{% include 'partials/home_meta.html' %}
<!-- Stylesheets -->
<link href="/static/css/bootstrap.min.css" rel="stylesheet">
<link href="/static/css/style.css" rel="stylesheet">
<link href="/static/css/responsive.css" rel="stylesheet">

<!-- Dark Theme -->
<link href="/static

<link rel="shortcut icon" href="/static/images/favicon.png" type="image/x-icon">
<link rel="icon" href="/static/images/favicon.png" type="image/x-icon">

<!-- Responsive -->
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">

</head>

<body>

<div class="page-wrapper">

	<!-- Preloader -->
	<div class="preloader"></div>

	<!-- Main Header-->
	<header class="main-header header-style-two">
		<!-- Header Top -->
		<div class="header-top">
			<div class="inner-container">

				<div class="top-left">
					<!-- Info List -->
					<ul class="list-style-one">
						<li><i class="fa fa-envelope"></i> <a href="mailto:{{ about.email|default:'o.jeff3.a@gmail.com' }}">{{ about.email|default:'o.jeff3.a@gmail.com' }}</a></li>
						<li><i class="fa fa-map-marker"></i> {{ about.address|default:'Ibrahim Shopping Mall, Moi Avenue, Nairobi' }}</li>
					</ul>
				</div>

				<div class="top-right">
					<ul class="useful-links">
						<li>
							<div class="form-check form-switch m-0 p-0">
								<label class="form-check-label my-0 py-0" for="flexSwitchCheckChecked">Dark Theme</label>
								<input class="form-check-input my-0 py-0" type="checkbox" {% if request.user.is_authenticated and request.user.preferences.theme == 'dark' %}checked{% endif %} role="switch" id="flexSwitchCheckChecked">
							</div>
						</li>
						<li><a href="{% url 'faq' %}">Help</a></li>
						{% if request.user.is_authenticated %}
							<li><a href="{% url 'users:profile' %}">Profile</a></li>
							<li><a href="{% url 'account_logout' %}">Logout</a></li>
						{% else %}				
							<li><a href="{% url 'account_signup' %}">Register</a></li>
							<li><a href="{% url 'account_login' %}">Login</a></li>
						{% endif %}
					</ul>
				</div>
			</div>

			<div class="outer-box">
				<ul class="social-icon-one">
					<li><a href="{{ about.twitter|default:'https://twitter.com/vstech_ltd' }}"><span class="fab fa-twitter"></span></a></li>
					<li><a href="{{ about.facebook|default:'https://fb.com' }}"><span class="fab fa-facebook-square"></span></a></li>
					<li><a href="{{ about.linkedin|default:'https://linkedin.com/company/vstech-ltd/' }}"><span class="fab fa-linkedin"></span></a></li>
					<li><a href="{{ about.youtube|default:'https://www.youtube.com/channel/UCkedAOBJuj9d0cvTKFzWqmA?sub_confirmation=1' }}"><span class="fab fa-youtube"></span></a></li>
				</ul>
			</div>
		</div>
		<!-- Header Top -->

		<!-- Header Lower -->
		<div class="header-lower">
			<!-- Main box -->
			<div class="main-box">
				<div class="logo-box">
					<div class="logo"><a href="/"><img src="/static/images/logo-2.png" alt="VSTech Limited" width="120" title="VSTech Limited"></a></div>
				</div>

				<!--Nav Box-->
				<div class="nav-outer">

					<nav class="nav main-menu">
						<ul class="navigation">
							<li class="{% if title == 'Homepage' %}current{% endif %}"><a href="/">Home</a></li>
							<li class="dropdown {% if title == 'About Us' or title == 'Projects' or title == 'Team Members' or title == 'Testimonies' or title == 'FAQs' %}current{% endif %}"><a href="#">About</a>
								<ul>
									<li><a href="{% url 'about' %}">About</a></li>
									<li><a href="{% url 'project:list' %}">Projects</a></li>
									<li><a href="{% url 'users:team' %}">Team</a></li>
									<li><a href="{% url 'testimonies' %}">Testimonial</a></li>
									<!-- <li><a href="page-pricing.html">Pricing</a></li>
									<li><a href="page-pricing-switcher.html">Pricing Switcher</a></li> -->
									<li><a href="{% url 'faq' %}">FAQ</a></li>
									
								</ul>
							</li>
							<li class="{% if title == 'Our Services' or category == 'services' %}current{% endif %}"><a href="{% url 'services:list' %}">Services</a></li>
							<li class="{% if title == 'Your Ultimate Electronics Shop' or category == 'Our Shop' %}current{% endif %}"><a href="{% url 'shop:list' %}">Shop</a></li>
							<li class="{% if title == 'Blogs' or category == 'Blogs > Test Category' %}current{% endif %}"><a href="{% url 'blog:list' %}">Blogs</a></li>
							<li class="{% if title == 'Contact Us' %}current{% endif %}"><a href="{% url 'contact' %}">Contact</a></li>
						</ul>
					</nav>
					<!-- Main Menu End-->


					<div class="outer-box">
						<div class="ui-btn-outer">
							<button class="ui-btn ui-btn search-btn">
								<span class="icon lnr lnr-icon-search"></span>
							</button>
							{% with total_items=cart|length %}
								<a href="{% url 'shop:cart' %}" class="ui-btn">
									<span class="d-flex no-wrap">
									<i class="lnr-icon-shopping-cart"></i>
									{% if total_items > 0 %}
										<sup class="text-success" style="font-size: 16px;">{{ total_items }}</sup>
									{% else %}
										<sup class="text-danger" style="font-size: 16px;">0</sup>
									{% endif %}
									</span>
								</a>
							{% endwith %}
						</div>


						<a href="tel:{{ about.phone_number|default:'+254795067776' }}" class="info-btn">
							<i class="icon fa fa-phone"></i>
							<small>Call Anytime</small><br> {{ about.phone_number|default:'+254795067776' }}
						</a>

						<a href="/static/VSTech Portfolio.pdf" class="theme-btn btn-style-one"><span class="btn-title">View porfolio</span></a>

						<!-- Mobile Nav toggler -->
						<div class="mobile-nav-toggler"><span class="icon lnr-icon-bars"></span></div>
					</div>
				</div>
			</div>
		</div>
		<!-- End Header Lower -->

		{% include 'partials/mobile_search_header.html' %}

	</header>
	<!--End Main Header -->
	{% include 'partials/messages.html' %}
